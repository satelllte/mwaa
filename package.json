  "repository": {
    "type": "git",
    "url": "git+https://github.com/satelllte/mwaa.git"
  },
  "bugs": {
    "url": "https://github.com/satelllte/mwaa/issues"
  },
  "homepage": "https://github.com/satelllte/mwaa#readme",
  "author": "@satelllte (https://github.com/satelllte)",
